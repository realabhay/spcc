def switcher(state,l):
 if state == 0:
    if l == 'a':
        return 1
    elif l == 'b':
        return 0
 elif state == 1:
    if l == 'a':
        return 1
    elif l == 'b':
        return 2
 elif state == 2:
    if l == 'a' :
        return 1
    elif l == 'b':
        return 0
        
state = 0
 
s = input("Enter the string: ")
for l in s:
 state = switcher(state,l)
print (state) 
if state == 2:
 print("String is accepted")
else :
 print("String is Rejected")
